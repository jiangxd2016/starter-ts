{
  "name": "pkg-name",
  "version": "0.0.0",
  "packageManager": "pnpm@9.0.4",
  "description": "",
  "type": "module",
  "keywords": [],
  "license": "MIT",
  "homepage": "https://github.com/estjs/pkg-name#readme",
  "bugs": "https://github.com/estjs/pkg-name/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/estjs/pkg-name.git"
  },
  "author": "jiangxd <jiangxd2016@gmail.com>",
  "funding": "https://github.com/sponsors/estjs",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint --cache --fix \"./src/*.{ts,tsx,js,vue}\"",
    "test": "vitest -r test",
    "typecheck": "tsc  --noEmit",
    "coverage": "vitest run --coverage",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --commit --tag --push && pnpm publish --access public"
  },
  "devDependencies": {
    "@estjs/eslint-config": "^1.1.1",
    "@vitest/coverage-v8": "^1.5.0",
    "bumpp": "^9.4.0",
    "dayjs": "^1.11.10",
    "eslint": "^8.57.0",
    "pnpm": "^9.0.4",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  }
}
